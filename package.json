  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "echo \"No tests configured yet\" && exit 0",
    "lint": "echo \"No linter configured yet\" && exit 0",
    "db:init": "bash scripts/init-db.sh",
    "db:migrate": "node scripts/run-migrations.js",
    "db:migrate:status": "node scripts/run-migrations.js status",
    "db:migrate:rollback": "node scripts/run-migrations.js rollback",
    "db:migrate:dry-run": "node scripts/run-migrations.js --dry-run",
    "db:sync": "node scripts/sync-scryfall.js",
    "stock:check": "node scripts/check-stock-alerts.js",
    "health": "curl -s http://localhost:3000/health | node -e \"process.stdin.pipe(process.stdout)\"",
